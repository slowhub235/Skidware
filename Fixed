        UIPadding.PaddingTop = UDim.new(0, 1)
        UIPadding.Parent = RightSection
        self._tab += 1
        if first_tab then
            self:update_tabs(Tab, LeftSection, RightSection)
            self:update_sections(LeftSection, RightSection)
        end
        Tab.MouseButton1Click:Connect(function()
            self:update_tabs(Tab, LeftSection, RightSection)
            self:update_sections(LeftSection, RightSection)
        end)
